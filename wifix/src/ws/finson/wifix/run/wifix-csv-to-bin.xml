<?xml version="1.0" encoding="UTF-8"?>
<config id="RecopyToBin" title="Process CSV to Binary">
    <application>
        <component type="ws.tuxi.lib.pipeline.ACPipelineEngine" classpath="ws.tuxi.lib.pipeline;ws.finson.wifix.app">
            <source type="CreateXMLDocument">
                <session>
                    <context>
                        <src-dir>raw</src-dir>
                        <dst-dir>post</dst-dir>
                    </context>
                </session>
            </source>
            <step type="ImportFromCSV">
                <file id="in" filename="sine-wave-reference.csv" />
            </step>
            <step type="SaveXMLDocumentToSink">
                <file />
            </step>
            <step type="ExportToCSV">
                <file extension="bin" />
                <file extension="csv" suffix="-mirror" />
                <nodes label-path="parameter-sequence/parameter[1]/@name" value-path="parameter-sequence/parameter[1]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[2]/@name" value-path="parameter-sequence/parameter[2]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[3]/@name" value-path="parameter-sequence/parameter[3]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[4]/@name" value-path="parameter-sequence/parameter[4]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[5]/@name" value-path="parameter-sequence/parameter[5]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[6]/@name" value-path="parameter-sequence/parameter[6]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[7]/@name" value-path="parameter-sequence/parameter[7]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[8]/@name" value-path="parameter-sequence/parameter[8]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[9]/@name" value-path="parameter-sequence/parameter[9]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[10]/@name" value-path="parameter-sequence/parameter[10]/parameter-values/value" />
                <nodes label-path="parameter-sequence/parameter[11]/@name" value-path="parameter-sequence/parameter[11]/parameter-values/value" />
            </step>
        </component>
    </application>
</config>