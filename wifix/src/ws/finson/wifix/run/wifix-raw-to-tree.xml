<?xml version="1.0" encoding="UTF-8"?>
<config id="ReadRawWiFi" title="Process Raw Wifi Data">
	<application>
		<component type="ws.tuxi.lib.pipeline.ACPipelineEngine"
			classpath="ws.tuxi.lib.pipeline;ws.finson.wifix.app">
			<source type="ImportCaptureScans">
				<file id="in">raw/data-file-1416332067.txt</file>
			</source>
			<step type="SendXMLToSink">
				<file>post/capture-file-1416332067.xml</file>
			</step>
			<step type="BuildSensorBranch">
				<key>BSSID</key>
				<sensor>RSSI</sensor>
				<sensor>CHANNEL</sensor>
				<sensor>SSID</sensor>
			</step>
			<step type="SendXMLDocumentToSink">
				<file>post/sensor-file-1416332067.xml</file>
			</step>
			<step type="BuildParameters">
				<parameter name="max-channel-strength">
					<expression>Max(RSSI)</expression>
					<key>CHANNEL</key>
				</parameter>
			</step>
			<step type="SendXMLDocumentToSink">
				<file>post/parameter-file-1416332067.xml</file>
			</step>
			<step type="WriteCSVFromColumnarXML">
				<file>post/spreadsheet-file-1416332067</file>
			</step>
		</component>
	</application>
</config>