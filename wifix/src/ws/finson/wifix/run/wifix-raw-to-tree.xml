<?xml version="1.0" encoding="UTF-8"?>
<config id="ReadRawWiFi" title="Process Raw Wifi Data">
	<application>
		<component type="ws.tuxi.lib.pipeline.ACPipelineEngine"
			classpath="ws.tuxi.lib.pipeline;ws.finson.wifix.app">
			<source type="ImportCaptureScans">
				<file id="in">raw/data-file-1416332067.txt</file>
			</source>
			<step type="SendXMLToSink">
				<file>post/capture-file-1416332067.xml</file>
			</step>
			<step type="BuildSensorBranch">
				<primary-key>BSSID</primary-key>
				<sensor>RSSI</sensor>
				<sensor>CHANNEL</sensor>
				<sensor>SSID</sensor>
			</step>
			<step type="SendXMLDocumentToSink">
				<file>post/analysis-file-1416332067.xml</file>
			</step>
			<step type="BuildParameterBranch">
				<parameter>RSSI</parameter>
				<parameter>Count(RSSI)</parameter>
				<parameter>Time</parameter>
				<parameter>DeltaTime</parameter>
				<parameter>Max(RSSI)</parameter>
				<parameter>Count(CHANNEL)</parameter>
			</step>
			<step type="WriteCSVFromColumnarXML">
				<file>post/processed-file-1416332067</file>
			</step>
		</component>
	</application>
</config>